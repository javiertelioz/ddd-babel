{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "mocha": true,
      "jest": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:prettier/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:promise/recommended"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2020
  },
  "rules": {
      "no-console": "off",
      "prettier/prettier": "error"
  }
}
